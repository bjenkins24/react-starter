{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "rules": {
    "radix": 0,
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2],
    "react/forbid-prop-types": [1, {"forbid":["any"]}],
    "jsx-a11y/label-has-for": [ 2, {
      "components": [ "Label" ],
      "required": {
        "every": [ "id" ]
      },
      "allowChildren": false,
    }],
    "jsx-a11y/href-no-hash": 0,
    "no-return-assign": 0,
    "no-alert": 1,
    "no-console": 1,
    "no-undef": 1,
    "react/jsx-filename-extension": [2, { "extensions": [".js"] }],
    "class-methods-use-this": 0,
    "react/jsx-no-bind": 0,
    "new-cap": 0,
    "react/require-default-props": 0,
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
